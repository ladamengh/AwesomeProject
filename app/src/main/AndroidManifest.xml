<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.awesomeproject">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".courses.CreateNewPartActivity"></activity>
        <activity android:name=".courses.FinishTestActivity" />
        <activity android:name=".courses.CreateQuestionsActivity" />
        <activity android:name=".courses.QuizActivity" />
        <activity android:name=".courses.CreateNewCourseActivity" />
        <activity android:name=".UserProfileActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".auth.MainActivity" />
        <activity
            android:name=".auth.LoginActivity"
            android:parentActivityName=".auth.MainActivity" />
        <activity
            android:name=".auth.SignUpActivity"
            android:parentActivityName=".auth.MainActivity" />
        <activity
            android:name=".messages.NewMessageActivity"
            android:parentActivityName=".messages.LatestMessagesActivity" />
        <activity
            android:name=".messages.LatestMessagesActivity"
            android:parentActivityName=".UserProfileActivity" />
        <activity
            android:name=".messages.ChatLogActivity"
            android:parentActivityName=".messages.LatestMessagesActivity" />
        <activity android:name=".courses.NewCourseActivity" />
        <activity android:name=".courses.CoursesListActivity" />
    </application>

</manifest>